    "jsonwebtoken": "^9.0.0",
    "knex": "^2.4.0",
    "morgan": "^1.10.0",
    "pg": "^8.8.0",
    "postgrator": "^7.1.1"
  },
  "devDependencies": {
    "chai": "^4.3.7",
